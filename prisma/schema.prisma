// prisma/schema.prisma

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id       Int       @id @default(autoincrement())
    name     String
    email    String    @unique
    phone    String?
    password String?
    date     DateTime?
}
